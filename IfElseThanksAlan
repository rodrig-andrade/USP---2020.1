#include <stdio.h>
#include <locale.h>

int main(){

    setlocale(LC_ALL, "Portuguese");

    char i = 'y';
    char resposta;

    printf("Você tem Computador? ");
    scanf("%c", &resposta);

    if (resposta == i){
        printf("Agradeça ao Alan Turing\n");
    } else {
        printf("Foda-se, agradeça ao Alan Turing");
    }

    return 0;
}
